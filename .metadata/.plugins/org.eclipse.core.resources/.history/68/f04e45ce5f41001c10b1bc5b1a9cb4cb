package Exercicios;

import java.util.Random;

public class Exercicio2 {
	public static void main(String[] args) {
		Random aleatorio = new Random();
		int valor = aleatorio.nextInt(4);
		int crescente = 0;
		System.out.println("Número gerado " + valor);
		
		if(valor >=  crescente) {
			crescente = valor;
		}
	}
	

}
