
public class Aula_5 {
	public static void main(String[] args) {

		int x;
		x = 150;

		int y = 200;
		int soma = x + y;
		int subtracao = x - y;
		int multiplicacao = x * y;

		System.out.println("Valor da soma: ", soma);
		System.out.println("Valor da subtração: ", subtracao);
		System.out.println("Valor da multiplicação: ", multiplicacao);
	}
}
